{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://*.akeneo.com"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://*.akeneo.com"
        }
      ]
    }
  ]
}
